<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout">
	<Dialog>
		<customHeader>
			<Bar class="hdbar">
				<contentMiddle>
					<Text text="{i18n>Check Out Verification}" class="hfrag"/>
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>
			<HBox>
				<Text text="{i18n>Are you sure you want to Check out?}" class="sapUiSmallMarginBegin sapUiSmallMarginTop txtco"/>  
			</HBox>
			<HBox justifyContent="End" class="sapUiSmallMarginTop">
				<Button text="{i18n>Yes}" type="Emphasized" width="3rem" press="onCheckOutFrag"/>
				<Button text="{i18n>No}" type="Reject" class="sapUiSmallMarginBeginEnd chbtn" press="onCancel" width="3rem"/>
			</HBox>
		</content>
	</Dialog>
</core:FragmentDefinition>