{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.demo.TimeandAttendance",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.demo.TimeandAttendance.view.Login",
			"type": "XML",
			"async": true,
			"id": "Login"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.demo.TimeandAttendance.i18n.i18n"
				}
			}
		},
		"Data": {
			"Data": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/date.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}, {
				"uri": "css/style2.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.demo.TimeandAttendance.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteLogin",
				"pattern": "RouteLogin",
				"target": [
					"TargetLogin"
				]
			}, {
				"name": "Signup",
				"pattern": "Signup",
				"target": [
					"Signup"
				]
			}, {
				"name": "Signup2",
				"pattern": "Signup2",
				"target": [
					"Signup2"
				]
			}, {
				"name": "Home",
				"pattern": "Home",
				"target": [
					"Home"
				]
			}, {
				"name": "Profile",
				"pattern": "Profile",
				"target": [
					"Profile"
				]
			}, {
				"name": "History",
				"pattern": "History",
				"target": [
					"History"
				]
			}, {
				"name": "Requests",
				"pattern": "Requests",
				"target": [
					"Requests"
				]
			}],
			"targets": {
				"TargetLogin": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Login"
				},
				"Signup": {
					"viewType": "XML",
					"viewName": "Signup"
				},
				"Signup2": {
					"viewType": "XML",
					"viewName": "Signup2"
				},
				"Home": {
					"viewType": "XML",
					"viewName": "Home"
				},
				"Profile": {
					"viewType": "XML",
					"viewName": "Profile"
				},
				"History": {
					"viewType": "XML",
					"viewName": "History"
				},
				"Requests": {
					"viewType": "XML",
					"viewName": "Requests"
				}
			}
		}
	}
}